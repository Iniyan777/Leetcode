int removeElement(int* nums, int numsSize, int val){
    if (numsSize == 0) {
        return numsSize;
    }
    size_t i, j = 0;
    int* temp = malloc(sizeof(int) * numsSize);

    for (i = 0; i < numsSize - 1; i++) {
        if (nums[i] != val) {
            temp[j++] = nums[i];
        }
    }

    if (nums[numsSize - 1] != val) {
        temp[j++] = nums[numsSize - 1];
    }

    for (i = 0; i < j; i++) {
        nums[i] = temp[i];
    }

    free(temp);
    return j;
}
